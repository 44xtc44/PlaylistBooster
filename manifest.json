{
  "version":"1.1",
  "author": "Ren√© Horn",
  "name":"Playlist Booster Shuffle [Volume Gain]",
  "short_name": "PlaylistBooster",
  "description": "Media player. Gain booster. Shuffle and play a folder with audio and video. [mp4, wbem, aac, mp3, ogg, ...] Time-seek and playback rate options.",
  "manifest_version":2,
  "homepage_url": "https://github.com/44xtc44/PlaylistBooster.git",
  "browser_action": {
    "default_icon": {
      "16": "/static/images/default_icon.svg",
      "32": "/static/images/default_icon.svg",
      "48": "/static/images/default_icon.svg",
      "96": "/static/images/default_icon.svg",
      "192": "/static/images/default_icon.svg",
      "384": "/static/images/default_icon.svg",
      "768": "/static/images/default_icon.svg",
      "1536": "/static/images/default_icon.svg"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
